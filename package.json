{
    "name": "k53ninja",
    "version": "0.0.1",
    "private": true,
    "description": "K53 Ninja App",
    "scripts": {
        "analyze:build": "webpack --progress --mode production --config webpack.production.js --profile --json > stats.json",
        "analyze:run": "webpack-bundle-analyzer stats.json",
        "build": "NODE_ENV='production' webpack --mode production --config webpack.production.js",
        "lint": "yarn lint:tsc && yarn lint:scripts",
        "lint-fix": "yarn lint-fix:scripts",
        "lint-fix:scripts": "yarn lint:scripts -- --fix",
        "lint:scripts": "eslint --max-warnings=10 \"src/**/*.{js,ts,tsx}\"",
        "lint:tsc": "yarn tsc",
        "start": "webpack-dev-server --mode development --config webpack.dev.js",
        "test": "jest --config=jest.config.json",
        "test:coverage": "jest --coverage --verbose --config=jest.config.json",
        "test:watch": "jest --watch --config=jest.config.json",
        "tsc": "tsc",
        "cap-sync": "cap sync",
        "cap-copy": "cap copy",
        "cap-sync-android": "cap sync android",
        "cap-copy-android": "cap copy android",
        "cap-sync-ios": "cap sync ios",
        "cap-copy-ios": "cap copy ios",
        "svg-to-component": "npx @svgr/cli --icon"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
